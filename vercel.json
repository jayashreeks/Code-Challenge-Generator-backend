{
    "version": 2,
    "builds": [
        {
        "src": "src/app.py",
        "use": "@vercel/python"
        }
    ],
    "routes": [
        {
        "src": "/api/(.*)",
        "dest": "/src/app.py"
        },
        {
        "src": "/webhooks/(.*)",
        "dest": "/src/app.py"
        }
    ]
}